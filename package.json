    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.17",
    "nodemailer": "^6.5.0",
    "pino": "^6.11.1",
    "uid": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
